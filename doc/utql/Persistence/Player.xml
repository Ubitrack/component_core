                    </Description>
                </Attribute>
            </Edge>
			
			
			<Edge name="OutputFinished" displayName="Finished signal" source="Event" destination="EventSpace">
				<Attribute name="type" value="Button" xsi:type="EnumAttributeReferenceType"/>
                <Attribute name="mode" value="push" xsi:type="EnumAttributeReferenceType"/>
			</Edge>
        </Output>
        <DataflowConfiguration>
            <UbitrackLib class="PlayerPose"/>
        </DataflowConfiguration>
    </Pattern>
    
